{
	"name": "subtle",
	"type": "dark",
	"colors": {
		"editor.background": "#1b1d1f",
		"editorCursor.foreground": "#b6b7b5",
		"editor.foreground": "#c2c5bd",
		"editorWhitespace.foreground": "#43464c",
		"editor.lineHighlightBackground": "#212325",
		"editor.selectionBackground": "#34373b"
	},
	"tokenColors": [
		{
			"name": "All",
			"scope": [
				"comment",
				"constant",
				"entity",
				"invalid",
				"keyword",
				"markup",
				"meta",
				"storage",
				"string",
				"support",
				"variable",
				"source"
			],
			"settings": {
				"foreground": "#34373b"
			}
		},
		{
			"name": "Show",
			"scope": [
				"string.quoted",
				"meta.paragraph",
				"entity.name.section"
			],
			"settings": {
				"foreground": "#c5c8c6"
			}
		},
		{
			"name": "Show LaTeX",
			"scope": [
				"text.tex.latex support.function.general.tex",
				"support.function.section.latex",
				"string.other.math.block.environment.latex",
				"string.other.math.block.environment.latex support.function.general.tex",
				"text.tex.latex string.other.math.tex",
				"text.tex.latex constant.other.general.math.tex",
				"text.tex.latex string.other.math.tex meta.group.braces.tex",
				"text.tex.latex invalid.illegal.string"
			],
			"settings": {
				"foreground": "#999b99"
			}
		},
		{
			"name": "Hide quoted strings in HTML/XML",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.double.xml"
			],
			"settings": {
				"foreground": "#34373b"
			}
		},
		{
			"name": "Markdown show",
			"scope": [
				"markup.raw.inline.markdown",
				"string.other.link.title.markdown",
				"string.other.link.description.markdown",
				"markup.quote.markdown"
			],
			"settings": {
				"foreground": "#c5c8c6"
			}
		},
		{
			"name": "Markdown show subtle",
			"scope": [
				"markup.underline.link.markdown",
				"markup.underline.link.image.markdown"
			],
			"settings": {
				"foreground": "#999b99"
			}
		},
		{
			"name": "Markdown hide",
			"scope": [
				"meta.image.inline.markdown",
				"meta.link.inline.markdown"
			],
			"settings": {
				"foreground": "#34373b"
			}
		},


    // MY CUSTOM OVERRIDES
		{
			"name": "Important genearl tokens that matter",
			"scope": [
					"support.constant",
					"support.class",
					"variable",
					"source.ocaml",
					"meta.paren-group.ocaml",
					"support.other.module.ocaml",
					"meta.module.structure.ocaml",
					"meta.record.ocaml",
					"entity.name.tag.js",
			],
			"settings": {
					"foreground": "#E0E0E0",
			}
	},
	{
			"name": "Special tokens that matter",
			"scope": [
					"storage.type.class.jsdoc",
					"punctuation.definition.block.tag.jsdoc",
					"meta.module-reference.ocaml",
					"constant.language.variant.ocaml",
			],
			"settings": {
					"foreground": "#4eeea3",
			}
	},
	{
			"name": "Tokens that we want to know about but not emphisize",
			"scope": [
					"entity.other.attribute-name",
					"string.quoted.single",
					"string.quoted.double",
					"string.template",
					"punctuation.definition.string",
					"support.variable.property",
					"constant.character.string.escape.ocaml",
			],
			"settings": {
					"foreground": "#9aa8b3",
			}
	},
	{
			"name": "Value tokens",
			"scope": [
					"constant.language.boolean",
					"constant.language.infinity",
					"keyword.other.storage.modifier.ocaml",
			],
			"settings": {
					"foreground": "#ffd580",
			}
	},
	{
			"name": "Grayed-out tokens",
			"scope": [
					"keyword.other.ocaml",
					"keyword.operator.arithmetic",
					"keyword.control.flow",
					"meta.object-literal",
					"entity.name.type",
					"constant.language.undefined",
					"constant.language.null",
					"meta.function",
					"keyword.operator",
					"punctuation.definition.string",
					"punctuation.definition",
					"meta.brace.square",
					"meta.brace.round",
					"meta.brace.square",
					"punctuation.terminator.statement",
					"meta.brace.round",
			],
			"settings": {
					"foreground": "#575d64",
			}
	},
	{
			"name": "Types",
			"scope": [
					"storage.type.ocaml",
					"support.type",
			],
			"settings": {
					"foreground": "#717981",
			}
	}
	],
}
